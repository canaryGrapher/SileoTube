{
    "manifest_version": 1,
    "name": "CalmTube",
    "version": "1.0",
    "description": "A calming, decluttered YouTube experience.",
    "icons": {
        "16": "icon/icon-16.png",
        "32": "icon/icon-32.png",
        "48": "icon/icon-48.png",
        "128": "icon/icon-128.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab"
    ]
}